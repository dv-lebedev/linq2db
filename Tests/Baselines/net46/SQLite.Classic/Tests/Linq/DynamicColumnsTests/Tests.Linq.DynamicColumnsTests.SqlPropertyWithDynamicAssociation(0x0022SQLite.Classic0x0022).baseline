-- SQLite.Classic SQLite

SELECT
	[x].[PersonID],
	[x].[FirstName],
	[x].[MiddleName],
	[x].[LastName]
FROM
	[Person] [x]
		LEFT JOIN [Patient] [a_Patient] ON [x].[PersonID] = [a_Patient].[PersonID]
WHERE
	[a_Patient].[Diagnosis] = 'Hallucination with Paranoid Bugs'' Delirium of Persecution'

